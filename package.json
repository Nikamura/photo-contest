{
  "name": "photo-contest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "bun run lint && bun run check",
    "build": "vite build",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy && prisma db seed && prisma generate",
    "postinstall": "npm install --no-save sharp && prisma generate",
    "preinstall": "npx only-allow bun",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@auth/core": "0.18.4",
    "@auth/prisma-adapter": "1.0.9",
    "@auth/sveltekit": "0.3.15",
    "@iconify-json/fa": "1.1.7",
    "@prisma/client": "5.6.0",
    "@sveltejs/adapter-vercel": "3.1.0",
    "@sveltejs/kit": "1.27.6",
    "@trigger.dev/sdk": "2.2.11",
    "@trigger.dev/sveltekit": "2.2.11",
    "@types/file-saver": "2.0.7",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@vercel/postgres": "0.5.1",
    "autoprefixer": "10.4.16",
    "bigger-picture": "1.1.12",
    "daisyui": "4.4.19",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "exif-reader": "2.0.0",
    "exiftool-vendored": "23.6.0",
    "file-saver": "2.0.5",
    "icc": "3.0.0",
    "iptc-reader": "1.0.1",
    "jszip": "3.10.1",
    "minio": "7.1.3",
    "postcss": "8.4.32",
    "postcss-import": "15.1.0",
    "postcss-load-config": "5.0.2",
    "prettier": "3.1.0",
    "prettier-plugin-svelte": "3.1.2",
    "prettier-plugin-tailwindcss": "0.5.7",
    "prisma": "5.6.0",
    "sharp": "0.32.6",
    "svelte": "4.2.8",
    "svelte-check": "3.6.2",
    "tailwindcss": "3.3.6",
    "tslib": "2.6.2",
    "tsx": "4.6.2",
    "typescript": "5.3.2",
    "unplugin-icons": "0.18.1",
    "vite": "4.5.1",
    "xmp-reader": "0.0.3"
  },
  "type": "module",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "trigger.dev": {
    "endpointId": "photo-contest"
  }
}
