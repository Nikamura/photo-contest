{
  "name": "photo-contest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "bun run lint && bun run check",
    "build": "vite build",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy && prisma db seed",
    "postinstall": "npm install --no-save sharp && prisma generate",
    "preinstall": "npx only-allow bun",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@auth/core": "0.16.1",
    "@auth/sveltekit": "0.3.9",
    "@aws-sdk/client-s3": "3.425.0",
    "@aws-sdk/s3-request-presigner": "3.425.0",
    "@iconify-json/fa": "1.1.5",
    "@prisma/client": "5.4.2",
    "@sveltejs/adapter-vercel": "3.0.3",
    "@sveltejs/kit": "1.25.1",
    "@trigger.dev/sdk": "2.2.0",
    "@trigger.dev/sveltekit": "2.2.0",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vercel/postgres": "0.5.0",
    "autoprefixer": "10.4.16",
    "aws-crt": "1.18.0",
    "daisyui": "3.9.2",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.34.0",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.5",
    "prisma": "5.4.2",
    "sharp": "0.32.6",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "unplugin-icons": "0.17.0",
    "vite": "4.4.11"
  },
  "type": "module",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "trigger.dev": {
    "endpointId": "photo-contest"
  }
}
